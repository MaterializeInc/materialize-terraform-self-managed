# Azure Authentication
# Set your Azure subscription ID (required)
ARM_SUBSCRIPTION_ID=your-azure-subscription-id

# Test Configuration
TEST_REGION=westus2

# Stage Control Variables
# Uncomment to skip specific stages during testing

# Skip infrastructure creation stages
# SKIP_setup_network=true
# SKIP_cleanup_network=true

# Disk Enabled
# SKIP_setup_aks_disk_enabled=true
# SKIP_setup_database_disk_enabled=true
# SKIP_setup_materialize_disk_enabled=true

# SKIP_cleanup_aks_disk_enabled=true
# SKIP_cleanup_database_disk_enabled=true
# SKIP_cleanup_materialize_disk_enabled=true

# Disk Disabled 
# SKIP_setup_aks_disk_disabled=true
# SKIP_setup_database_disk_disabled=true
# SKIP_setup_materialize_disk_disabled=true

# SKIP_cleanup_aks_disk_disabled=true
# SKIP_cleanup_database_disk_disabled=true
# SKIP_cleanup_materialize_disk_disabled=true

# test will try to use latest existing network in the following two scenarios
# 1. if SKIP_setup_network is unset, and USE_EXISTING_NETWORK is set (this is when the network has some issue during apply and you want to re-apply the fixes instead of creating a new one)
# 2. if SKIP_setup_network is set
# USE_EXISTING_NETWORK=true
