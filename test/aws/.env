# AWS Test Environment Configuration
# Copy this file to .env and update with your values

# Project Configuration
# PROJECT_ROOT=/path/to/materialize-terraform-self-managed

# AWS Configuration
AWS_REGION=us-west-2
AWS_PROFILE=<your-aws-profile> # Ensure this profile has the necessary permissions

# Alternative: Use access keys instead of profile
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key
# AWS_SESSION_TOKEN=your-session-token  # Optional, for temporary credentials

# Terraform Debugging (Optional)
# TF_LOG=INFO
# TF_LOG_PATH=terraform-debug.log

# Stage Skip Configuration (Optional)

# USE_EXISING_NETWORK=true # This flag will only take effect if SKIP_setup_network is unset

# SKIP_setup_network=true
# SKIP_cleanup_network=true

# SKIP_setup_database=true
# SKIP_cleanup_database=true

# SKIP_setup_storage=true
# SKIP_setup_eks=true
# SKIP_setup_materialize=true
